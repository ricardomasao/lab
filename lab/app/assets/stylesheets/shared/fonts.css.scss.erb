@font-face {
  font-family: "ChaletParisSixty";
  src: url(<%= asset_path('Chalet-ParisSixty.eot')%>);
  src: url("<%= asset_path('Chalet-ParisSixty.eot?#iefix')%>") format("embedded-opentype"),
       url("<%= asset_path('Chalet-ParisSixty.woff')%>") format("woff"),
       url("<%= asset_path('Chalet-ParisSixty.ttf')%>") format("truetype"),
       url("<%= asset_path('Chalet-ParisSixty.svg#Chalet-ParisSixty')%>") format("svg");
  font-weight: normal;
  font-style: normal;
}

